<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>True</Beta>
  <Name>Icecast</Name>
  <Description>
    Icecast is a streaming media server which currently supports Ogg (Vorbis and Theora), Opus, WebM and MP3 audio streams.
  </Description>
  <Overview>
    Icecast is a streaming media server which currently supports Ogg (Vorbis and Theora), Opus, WebM and MP3 audio streams.
  </Overview>
  <Support></Support>
  <Registry>https://hub.docker.com/r/moul/icecast</Registry>
  <GitHub>https://github.com/moul/docker-icecast</GitHub>
  <Repository>moul/icecast</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8000</HostPort>
        <ContainerPort>8000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>ICECAST_SOURCE_PASSWORD</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>ICECAST_ADMIN_PASSWORD</Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Icon>https://wiki.xiph.org/images/thumb/e/ef/Icecast_Logo.svg/100px-Icecast_Logo.svg.png</Icon>
</Container>
